<module version="2.0">
	<meta>
		<identity>
			<uri>urn:net:bosatsu:presentations:shacl</uri>
			<version>1.1.1</version>
		</identity>
		<info>
			<name>Bosatsu / Slides / SHACL</name>
			<description>SHACL Slides</description>
		</info>
	</meta>
	<system>
		<dynamic/>
	</system>
	
	<rootspace>
		<fileset>
			<regex>res:/etc/system/SimpleDynamicImportHook.xml</regex>
		</fileset>
		<mapper>
			<config>
				<import>res:/content/mapper.xml</import>
			</config>
			<space>
				<fileset>
					<regex>res:/content/.*</regex>
				</fileset>
				<!--Core Wiki Libs-->				
				<import>
					<uri>urn:org:netkernel:wiki:core</uri>
				</import>
				<import>
					<uri>urn:org:netkernel:wiki:slinki</uri>
				</import>
				<import>
					<uri>urn:org:netkernel:wiki:macros</uri>
				</import>
				
				<!--Various Libs-->
				<import>
					<uri>urn:org:netkernel:lang:xrl</uri>
				</import>
				<import>
					<uri>urn:org:netkernel:lang:trl</uri>
				</import>
				<import>
					<uri>urn:org:netkernel:ext:layer1</uri>
				</import>
				<import>
					<uri>urn:org:netkernel:lang:groovy</uri>
				</import>
				<import>
					<uri>urn:org:netkernel:tpt:http</uri>
				</import>
				<import>
					<uri>urn:org:netkernel:xml:core</uri>
				</import>
				
			</space>
		</mapper>
	</rootspace>
	
	
</module>
